{
  "name": "b2b-extension-custom-app",
  "version": "1.0.0",
  "description": "Merchant Center B2B Custom App",
  "private": true,
  "scripts": {
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod:local": "yarn compile-html:local && mc-scripts serve",
    "lint": "eslint . ",
    "lint-fix": "eslint --fix .",
    "lint-md": "remark -f README.md",
    "format": "yarn format:js & yarn format:css & yarn format:md & yarn format:yaml",
    "format:js": "prettier --write '**/*.js'",
    "format:css": "prettier --write --parser css '**/*.css'",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:graphql": "prettier --write --parser graphql '**/*.graphql'",
    "format:yaml": "prettier --write --parser yaml '**/*.{yaml,yml}'",
    "i18n:build": "mc-scripts extract-intl --output-path=$(pwd)/src/i18n/data 'src/**/!(*.spec).js' --build-translations",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "compile-html:firebase": "mc-scripts compile-html --transformer $(pwd)/config/transformer-firebase.js",
    "compile-html:local": "NODE_ENV=production MC_APP_ENV=development dotenv -- mc-scripts compile-html --transformer @commercetools-frontend/mc-dev-authentication/transformer-local.js"
  },
  "dependencies": {
